{
  "manifest_version": 2,
  "name": "React Content Script",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": [
        "./static/js/main.js"
      ],
      "css": ["./static/css/main.css"],
      "media": []
    }
  ],
  "permissions": ["storage","activeTab"],
  "content_security_policy":  "script-src 'self' blob: 'unsafe-eval' 'wasm-eval'; object-src 'self'"
}
